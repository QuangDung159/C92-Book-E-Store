{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "local",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "preview.qa": {
      "distribution": "internal",
      "channel": "preview.qa",
      "env": {
        "ENV_FILE": ".env.qa",
        "EXPO_PUBLIC_ENV": "QA",
        "EXPO_PUBLIC_BASE_URL": "https://nodejs-e-commerce-dev.onrender.com/"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "preview.stg": {
      "distribution": "internal",
      "channel": "preview.stg",
      "env": {
        "ENV_FILE": ".env.stg",
        "EXPO_PUBLIC_ENV": "STG",
        "EXPO_PUBLIC_BASE_URL": "https://nodejs-e-commerce-dev.onrender.com/"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "preview.prod": {
      "distribution": "internal",
      "channel": "production",
      "env": {
        "ENV_FILE": ".env.prod",
        "EXPO_PUBLIC_ENV": "PROD",
        "EXPO_PUBLIC_BASE_URL": "https://nodejs-e-commerce.onrender.com/"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "ENV_FILE": ".env.dev",
        "EXPO_PUBLIC_ENV": "DEV",
        "EXPO_PUBLIC_BASE_URL": "https://nodejs-e-commerce-dev.onrender.com/"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "env": {
        "ENV_FILE": ".env.prod",
        "EXPO_PUBLIC_ENV": "PROD",
        "EXPO_PUBLIC_BASE_URL": "https://nodejs-e-commerce.onrender.com/"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6748847426"
      }
    }
  }
}