{
  "cli": {
    "version": ">= 10.2.3",
    "appVersionSource": "local"
  },
  "build": {
    "preview.qa": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {},
      "channel": "preview.qa"
    },
    "preview.stg": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {},
      "channel": "preview.stg"
    },
    "preview.prod": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "ios": {},
      "channel": "preview.prod"
    },
    "development": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "channel": "development"
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "env": {
        "ENV_FILE": ".env.prod",
        "EXPO_PUBLIC_ENV": "PROD",
        "EXPO_PUBLIC_BASE_URL": "http://103.121.89.190:27894/"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "local"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    }
  },
  "submit": {
    "production": {}
  }
}