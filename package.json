{
  "name": "c92_book_e_store",
  "main": "index.js",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "set:env:backup": "node ./scripts/set-env-backup.js",
    "set:env:revert": "node ./scripts/set-env-revert.js",
    "set:env:prod:build": "node ./scripts/set-env-prod.js",
    "set:env:qa:build": "node ./scripts/set-env-qa.js",
    "set:env:stg:build": "node ./scripts/set-env-stg.js",
    "set:env:dev:build": "node ./scripts/set-env-dev.js",
    "set:env:prod": "npm run set:env:prod:build && npm run set:env:backup",
    "set:env:qa": "npm run set:env:qa:build && npm run set:env:backup",
    "set:env:stg": "npm run set:env:stg:build && npm run set:env:backup",
    "set:env:dev": "npm run set:env:dev:build && npm run set:env:backup",
    "build:android:qa:local": "npm run set:env:qa:build && dotenv -- eas build -p android --profile preview.qa --local && npm run set:env:revert",
    "build:android:stg:local": "npm run set:env:stg:build && dotenv -- eas build -p android --profile preview.stg --local && npm run set:env:revert",
    "build:android:prod:local": "npm run set:env:prod:build && dotenv -- eas build -p android --profile preview.prod --local && npm run set:env:revert",
    "build:android:development": "npm run set:env:qa && dotenv -- eas build -p android --profile development",
    "build:android:development:local": "npm run set:env:qa && dotenv -- eas build -p android --profile development --local && npm run set:env:revert",
    "build:aab": "eas build -p android --profile production",
    "build:aab:local": "npm run set:env:prod:build && dotenv -- eas build -p android --profile production --local && npm run set:env:revert",
    "build:ios:dev": "eas build -p ios --profile development",
    "build:nodev": "npx expo start --no-dev",
    "eas:update:qa": "npm run set:env:qa && eas update --branch preview.qa --message \"$npm_config_message\" && npm run set:env:revert",
    "eas:update:stg": "npm run set:env:stg && eas update --branch preview.stg --message \"$npm_config_message\" && npm run set:env:revert",
    "eas:update:prod": "npm run set:env:prod && eas update --branch production --message \"$npm_config_message\" && npm run set:env:revert",
    "prebuild": "expo prebuild",
    "build:android:qa:local:test": "npm run set:env:qa:build && npm run set:env:revert"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/in-app-messaging": "^20.4.0",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/material-top-tabs": "^6.6.14",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@shopify/flash-list": "1.6.4",
    "axios": "^1.7.4",
    "crypto-js": "^4.2.0",
    "expo": "~51.0.24",
    "expo-auth-session": "^5.5.2",
    "expo-build-properties": "^0.12.5",
    "expo-clipboard": "^6.0.3",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.23",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.9",
    "expo-image": "~1.12.13",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.16",
    "expo-router": "~3.5.20",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "^0.25.23",
    "expo-web-browser": "~13.0.3",
    "mobx": "^6.13.1",
    "mobx-react-lite": "^4.0.7",
    "onesignal-expo-plugin": "^2.0.3",
    "react": "18.2.0",
    "react-devtools-core": "^5.3.1",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-collapsible": "^1.6.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-fbsdk-next": "^13.0.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-modal": "^13.0.1",
    "react-native-onesignal": "^5.2.3",
    "react-native-pager-view": "^6.4.1",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-reanimated-carousel": "^4.0.0-alpha.11",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.1",
    "react-native-star-rating-widget": "^1.8.0",
    "react-native-svg": "15.2.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "redux-saga": "^1.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
